{
  "expo": {
    "name": "Maaura",
    "slug": "maaura",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "web": {
      "build": {
        "babel": {
          "plugins": [
            "react-native-web"
          ]
        }
      },
      "favicon": "./assets/favicon.png",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      },
      "browserReload": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.maaura.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.maaura.app"
    },
    "scheme": "maaura",
    "extra": {
      "eas": {
        "projectId": "60029b43-43c6-4139-89d3-03a7b2a829a2"
      }
    }
  }
}
